Git live
1.git branch 
2.live folder first before your code added git pull
3.demo code ng build --c live cut 
4.alreay live build_old delete current build 
Rename build_old finally live build add
5.git add .
6.git commit -m "update"
7.git pull
8.git push 




exports.getContest = (req, res) => {
  var contestId = req.body.contestId;
  USERDETAILS.findOne({ contestId: contestId }, function (err, data) {
    if (err) {
      console.log("error: ", err);
    } else if (data == null) {
      USERDETAILS.find({ contestId: { $lt: contestId } })
        .sort([["contestId", -1]]).limit(1)
        .exec(function (err, datas) {
          res.json({ status: true, message: "show", data: datas });
        });
    } else if (data != null) {
      res.json({ status: true, message: "success", data: data });
    }
  });
};
